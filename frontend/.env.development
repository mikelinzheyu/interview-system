# AI智能面试系统 - 开发环境配置
VITE_APP_TITLE=AI智能面试系统
VITE_APP_VERSION=2.0.0
VITE_APP_ENV=development

# API配置
VITE_API_BASE_URL=/api
VITE_API_TIMEOUT=30000
VITE_WS_BASE_URL=auto
VITE_DEV_PROXY_TARGET=http://localhost:3001

# 功能开关
VITE_ENABLE_MOCK=false
VITE_USE_MOCK_DATA=false
VITE_ENABLE_ERROR_BOUNDARY=true
VITE_ENABLE_API_RETRY=true
VITE_ENABLE_HEALTH_MONITOR=true
VITE_ENABLE_PERFORMANCE_MONITOR=true

# 通知与提示行为
VITE_HEALTH_NOTIFY_AUTO_CLOSE=true
VITE_HEALTH_NOTIFY_DURATION=8000
VITE_ENABLE_FLOATING_NOTIFICATIONS=false

# 日志配置
VITE_LOG_LEVEL=debug
VITE_ENABLE_CONSOLE_LOG=true

# 开发工具配置
VITE_ENABLE_DEVTOOLS=true
VITE_ENABLE_HOT_RELOAD=true

# 媒体设备配置
VITE_CAMERA_QUALITY=hd
VITE_AUDIO_QUALITY=high
VITE_ENABLE_DEVICE_FALLBACK=true

# AI功能配置
VITE_AI_QUESTION_CACHE=true
VITE_AI_ANALYSIS_TIMEOUT=60000
VITE_AI_FALLBACK_ENABLED=true

# 安全配置
VITE_CSRF_ENABLED=true
VITE_XSS_PROTECTION=true
